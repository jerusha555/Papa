  <script>
    //faq's
    var base_url = "https://naanmudhalvan.smartinternz.com/";
    var acc = document.getElementsByClassName("accordion");
    var i;

    for (i = 0; i < acc.length; i++) {
      acc[i].addEventListener("click", function() {
        this.classList.toggle("faq-active");
        var panel = this.nextElementSibling;
        if (panel.style.maxHeight) {
          panel.style.maxHeight = null;
        } else {
          panel.style.maxHeight = panel.scrollHeight + "px";
        }
      });
    }

    $(document).ready(function() {
      $('#forgotpassword_tabs').click(function() {
        $('#student_ibm_login').hide();
        $('#forgot_form').show();
      })
      $('#login_tabs').click(function() {
        $('#student_ibm_login').show();
        $('#forgot_form').hide();
      })
      blur_functions();
      // Add minus icon for collapse element which is open by default
      $(".collapse.show").each(function() {
        $(this).prev(".card-header").find(".fa").addClass("fa-minus").removeClass("fa-plus");
      });

      // Toggle plus minus icon on show hide of collapse element
      $(".collapse").on('show.bs.collapse', function() {
        $(this).prev(".card-header").find(".fa").removeClass("fa-plus").addClass("fa-minus");
      }).on('hide.bs.collapse', function() {
        $(this).prev(".card-header").find(".fa").removeClass("fa-minus").addClass("fa-plus");
      });
    });



    var base_url = "https://naanmudhalvan.smartinternz.com/";
    var proceed_payment = "";
    $(function() {
      blur_functions();

      $('#country_code').change(function() {
        var country_code = $('#country_code').val();
        $('#c_code').html('+' + country_code);
      })
      $('#forgot_form').submit(function() {
        var validatons = 1;

        var activation_username = $('#activation_username').val();
        if (activation_username == '') {
          $('#activation_username').css('border', '1px solid red');
          validatons = 0;
        } else {
          $('#activation_username').css('border', '');
        }


        if (validatons == 1) {
          $.ajax({
            url: "Welcome/generic_student_login",
            data: new FormData(this),
            type: 'POST',
            processData: false,
            contentType: false,
            success: function(data) {
              var data = $.parseJSON(data);
              if (data.err == 1) {
                $('#activation_username').css('border', '');
                $('#activation_username').val('');
                alertify.notify("Please check your email or mobile for temporary password");
                $('#student_ibm_login').show();
                $('#forgot_form').hide();
              } else if (data.err == 2) {
                alertify.notify("Incorrect Login/Password");
              } else {
                alertify.notify("Please check your Internet connection");
              }
            }
          })

        }
      });
      $('#duration').change(function() {
        var duration = $(this).val();
        if (duration == '4 Weeks') {
          $('#currency2').html('INR 1999');
          $('#merchant_total').val(1999 * 100);
          $('#merchant_amount').val(1999);
        } else if (duration == '6 Weeks') {
          $('#currency2').html('INR 1999');
          $('#merchant_total').val(1999 * 100);
          $('#merchant_amount').val(1999);
        } else if (duration == '8 Weeks') {
          $('#currency2').html('INR 2449');
          $('#merchant_total').val(2449 * 100);
          $('#merchant_amount').val(2449);
        } else if (duration == '10 Weeks') {
          $('#currency2').html('INR 2999');
          $('#merchant_total').val(2999 * 100);
          $('#merchant_amount').val(2999);
        }
      })

    });

    $('#forgotpassword_tabs').click(function() {
      $('#student_ibm_login').hide();
      $('#forgot_form').show();
