    $('#student_ibm_login').on('submit', function(e) {
      var validatons = 1;

      $('.login_fom').each(function() {
        var valuee = $(this).val();
        if (valuee == '') {
          $(this).css('border-color', 'red');
          validatons = 0;
        } else {
          $(this).css('border', '');
        }
      })


      var login_password = $('#login_password').val();
      var studentName = $('#username').val();
      if (login_password == '') {
        $('#flexx').css('border', '1px solid red');
        validatons = 0;
      } else {
        $('#flexx').css('border', '');
      }
      if (grecaptcha.getResponse() == "") {
        e.preventDefault();
        validatons = 0;
        $('#alert_recaptcha').html('<p style="color:red">Please confirm the Captcha</p>');
      } else {
        $('#alert_recaptcha').html('');
      }
      if (validatons == 1) {
        $.ajax({
          url: base_url + "Welcome/generic_student_login",
          data: new FormData(this),
          type: 'POST',
          processData: false,
          contentType: false,
          success: function(data) {
            var data = $.parseJSON(data);
            // alert(studentName+'-------'+login_password);
            if (data.err == 1) {
              if (mob_web == 'web') {
                $('.login_fom').each(function() {
                  $(this).css('border-color', '');
                  $('#flexx').css('border', '');
                  $(this).val('');
                });
              } else {
                $('.login_form_mob').each(function() {
                  $(this).css('border-color', '');
                  $('#flexx').css('border', '');
                  $(this).val('');
                });
              }
              $('#login_password').val('');
              alertify.success("Login Successful !!");

              // var previouse_link = document.referrer;
              // const SSOProvider = "https://sso.smartinternz.com/";
              // let LMSData = {
              //     "LMSTargetAction": "SSOLogin",
