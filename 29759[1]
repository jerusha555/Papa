              // SSOApp.contentWindow
              //     .postMessage(
              //         JSON.stringify(LMSData),
              //         SSOProvider
              //     );
              // alert('2');
              window.location.replace(base_url + "Student/Profile");

            } else if (data.err == 2) {
              alertify.error("Your email/password are not matched..");
            } else if (data.err == 3) {
              alertify.error("This account is not activated..");
            } else {
              alertify.warning("Please check your Internet connection");
            }
          }
        })

      }
    });
